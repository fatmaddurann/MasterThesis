{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://visionsleuth-ai-backend.onrender.com",
    "NEXT_PUBLIC_WS_URL": "wss://visionsleuth-ai-backend.onrender.com/ws"
  }
} 